{
    "name": "mpetrini/doctrinedatatable",
    "description": "Bridge between jQuery Datatable ServerSide and Doctrine ORM",
    "keywords": ["datatable", "doctrine", "jquery"],
    "type": "library",
    "require": {
        "php": "^7.1",
        "doctrine/dbal": "^2.4",
        "doctrine/orm": "^2.4"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.0",
        "friendsofphp/php-cs-fixer": "^2.14@dev",
        "pheromone/phpcs-security-audit": "dev-master",
        "sensiolabs/security-checker": "^5.0@dev"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "mpetrini",
            "email": "mathieupetrini@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "DoctrineDatatable\\": "src/DoctrineDatatable/"
        },
        "exclude-from-classmap": [
            "**/Tests/"
        ]
    },
    "minimum-stability": "stable"
}
